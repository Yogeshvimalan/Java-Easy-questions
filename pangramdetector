package pangramdetector;

public class pangramdetector1 {
	private static String findMissingLetters(String string) {

		int index = 0;
		for (int i = 0; i < string.length(); i++) {
		if( string.charAt(i) >= 'a' && string.charAt(i) <= 'z') {
		      index = string.charAt(i) - 'a';
		   }
		
	}
		return null;
	}
	   public static void main(String []args) {
	    
	    Boolean success = true;
	    success = success && "".equalsIgnoreCase(findMissingLetters("The quick brown fox jumps over the lazy dog"));
	    
	    if(success) {
	      System.out.println("All test passed");
	    }
	    else {
	      System.out.println("There is a test failure");
	    }
	  }

	
	}
